{{#if products.length}}
<table class="table table-dark table-striped">
    <tr>
        <th>nombre</th>
        <th>precio</th>
        <th>imagen</th>
    </tr>
    {{#each products}}
    <tr class="table-primary">
        <td class="table-dark">{{this.title}}</td>
        <td class="table-dark">{{this.price}}</td>
        <td class="table-dark"> <img style="max-width:100px ;" src={{this.thumbnail}} /></td>

    </tr>
        {{/each}}
</table>
{{else}}
<h3>NO HAY PRODUCTOS CARGADOS</h3>

{{/if}}